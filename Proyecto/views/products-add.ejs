<!DOCTYPE html>
<html lang="es">
<%-include("./partials/head.ejs")%>

<body>
	<% if (usuario!= null) { %>
		<%- include("./partials/header.ejs") %>
	<% } else { %>
		<%- include("./partials/headerLogueado.ejs")%>
	<% } %>
	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Agregar el producto al Carrito ðŸ›’ </h2> 
		</div>
		
	</div>
	<form method="post" enctype="multipart/form-data" >
		<div class="mb-3">					
			<label class="form-label">Marca</label>
			<input type="text" class="form-control" name="marca" id="marca" />
		  </div>
		  <div class="mb-3">
			<label class="form-label">Modelo</label>
			<input type="text" class="form-control" name="modelo" id="modelo" />	
		  </div>
		  <div class="mb-3">
			<label class="form-label">Descripcion</label>
			<input type="text" class="form-control" name="descripcion" id="descripcion" />
		  </div>
		  <div class="mb-3">
			<label for="perfil" class="form-label">Imagen</label>
			<input type="file" class="form-control" name="imagenes" id="imagenes">
		  </div>
		
	  
		
	  
		
	  
		<button type="submit" class="btn btn-primary">Submit</button>
	  </form>
	<%-include("./partials/footer.ejs")%>
</body>
</html>
