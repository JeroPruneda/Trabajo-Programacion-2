<!DOCTYPE html>
<html lang="es">
<!-- Include Head -->
<%-include("./partials/head.ejs")%>
<body>
	<%-include("./partials/header.ejs")%>
	<!-- Banner -->
	<!-- <div class="banner">
		<img src="images/img-home-banner.jpg" alt="Banner de ofertas">
	</div> -->
	<!-- /Banner -->

	<!-- Novedades -->
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="products-title">Novedades</h2>
			</div>
		
		<% for(i=0, i < data.productos.length,i++){}
			<!-- Show the "ultimos" products -->
			<div class="col-12 col-sm-6 col-lg-3">
				<section class="product-box">
					<a href="/productoDetalle">
						
						<figure class="product-box_image">
							<img src="<%= data.productos[1].imagen%>"" alt="cafetera moulinex">
						</figure>
						<article class="product-box_data">
							<h2><%= data.productos[1].marca %></h2>
							<p><%= data.productos[1].descripcion %></p>
							<p>Comentarios: 12</p>

						
						</article>
					</a>
				</section>
			</div>
			
	<!-- /MÃ¡s comentados -->
			
	<%-include("./partials/footer.ejs")%>
</body>	

