			<input type="date" class="form-control" id="exampleInputFecha" name="fecha_de_nacimiento">
		</div>
		<div class="mb-3">
			<label for="documento" class="form-label">Numero de documento</label>
			<input type="number" class="form-control" id="documento" name="documento">
		</div>
		<label for="perfil">Foto de Perfil</label>
		<input type="file" class="form-control" name="perfil" id="perfil" enctype="multipart/form-data" />
		<button type="submit" class="btn btn-primary">Submit</button>
	  </form>
